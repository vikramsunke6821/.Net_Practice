@model List<MVCCrud.Models.StudentModel>

@{
    ViewBag.Title = "Index";
}
<h2>Student Details</h2>

<table class="table table-striped table-bordered">
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Email</th>
        <th>Age</th>
        <th>Number</th>
    </tr>

    @foreach(var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.Email</td>
            <td>@item.Age</td>
            <td>@item.Number</td>

        </tr>
    }

</table>