@model EFDemo.Models.Domain.Employee

<form method="post" Action="Put">
    <div class="mt-3">
        <input type="hidden" class="form-control" asp-for="Id">
    </div>
    <div class="mt-3">
        <label class="form-label" for="">Name</label>
        <input type="text" class="form-control" asp-for="Name">
    </div>
    <div class="mt-3">
        <label class="form-label" for="">Email</label>
        <input type="email" class="form-control" asp-for="Email">
    </div>
    <div class="mt-3">
        <label class="form-label" for="">Age</label>
        <input type="number" class="form-control" asp-for="Age">
    </div>
    <div class="mt-3">
        <label class="form-label" for="">Number</label>
        <input type="number" class="form-control" asp-for="Number">
    </div>
    <div class="mt-3">
        <button type="submit" class="btn btn-primary">Update</button>

    </div>
</form>


